
all:
	@(cd QtGui; $(MAKE))
	@(cd QtDesigner; $(MAKE))
	@(cd QtOpenGL; $(MAKE))
	@(cd QtCore; $(MAKE))
	@(cd QtDBus; $(MAKE))
	@(cd QtDeclarative; $(MAKE))

install:
	@(cd QtGui; $(MAKE) install)
	@(cd QtDesigner; $(MAKE) install)
	@(cd QtOpenGL; $(MAKE) install)
	@(cd QtCore; $(MAKE) install)
	@(cd QtDBus; $(MAKE) install)
	@(cd QtDeclarative; $(MAKE) install)

clean:
	@(cd QtGui; $(MAKE) clean)
	@(cd QtDesigner; $(MAKE) clean)
	@(cd QtOpenGL; $(MAKE) clean)
	@(cd QtCore; $(MAKE) clean)
	@(cd QtDBus; $(MAKE) clean)
	@(cd QtDeclarative; $(MAKE) clean)
